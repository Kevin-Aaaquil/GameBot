const db = require('./../data');
const _db = db.getDb();
async function test() {
  try {
    let i = 1; //use a random operator here
    let data = await _db.collection('truth').findOne({ index: i });
    console.log(data.statement);
    return data.statement;
  } catch (error) {}
}

module.exports = {
  name: '',
